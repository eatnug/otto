{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Otto",
  "version": "0.1.0",
  "identifier": "com.otto.dev",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Otto",
        "width": 680,
        "height": 88,
        "decorations": false,
        "transparent": true,
        "alwaysOnTop": true,
        "skipTaskbar": true,
        "center": true,
        "visible": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [],
    "macOS": {
      "signingIdentity": "Apple Development: Jacob Park (9WMH775RUJ)"
    }
  },
  "plugins": {}
}
